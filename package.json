{
  "name": "web-starter",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch index.ts",
    "build": "npm run build:src && npm run build:ssr && npm run build:server",
    "build:src": "vite build",
    "build:ssr": "vite build -c vite.ssr.config.ts",
    "build:lib": "vite build -c vite.lib.config.ts",
    "build:server": "rimraf dist/index.js dist/server && tsc -p tsconfig.server.json",
    "preview": "cross-env NODE_ENV=production node dist/index.js",
    "prepublishOnly": "npm run build:lib"
  },
  "dependencies": {
    "axios": "^1.13.5",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "sirv": "^3.0.2"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/css-modules": "^1.0.5",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.2",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "cross-env": "^10.1.0",
    "rimraf": "^6.1.2",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4"
  }
}
